project 'THMReader.xcodeproj'

# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'THMReader' do
  platform :ios, '10.0'
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  # Pods for THMReader
  pod 'FeedKit', '~> 5.0'

end

target 'THMReader WatchKit App' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for THMReader WatchKit App
  pod 'FeedKit', '~> 5.0'

end

target 'THMReader WatchKit Extension' do
  platform :watchos, '2.0'
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for THMReader WatchKit Extension
  pod 'FeedKit', '~> 5.0'

end
